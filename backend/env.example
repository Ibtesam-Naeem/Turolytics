# Turolytics Environment Configuration
# Copy this file to .env and fill in your actual values

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/turolytics
USE_SQLITE=false

# Server Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=true
SECRET_KEY=your-secret-key-here

# Turo Credentials
TURO_EMAIL=your-email@example.com
TURO_PASSWORD=your-password

# Bouncie API Configuration
BOUNCIE_CLIENT_ID=your-bouncie-client-id
BOUNCIE_CLIENT_SECRET=your-bouncie-secret
BOUNCIE_REDIRECT_URI=http://localhost:8000/auth/bouncie/callback

# Plaid API Configuration
PLAID_CLIENT_ID=your-plaid-client-id
PLAID_SECRET=your-plaid-secret
PLAID_ENV=sandbox

# Scraping Configuration
SCRAPING_TIMEOUT=300
SCRAPING_HEADLESS=true
MAX_CONCURRENT_TASKS=5

# ML Service Configuration
ML_SERVICE_PORT=8001
ML_MODEL_PATH=./models/
